		<= Hint $mol_string
			hint @ \Batman
			value?val <=> name?val \
		<= Filled $mol_string
			value?val <=> name2?val \Jocker
		<= Disabled $mol_string
			disabled true
			value?val <=> name2?val -

$mol_suggest $mol_search
	query?val <=> value?val \

$mol_switch_demo $mol_demo_small
	title @ \Color switchers in various state
	sub /
		<= Enabled $mol_switch
			value?val <=> color?val \red
			options *
				red <= option_red @ \Red
				green <= option_green @ \Green
				blue <= option_blue @ \Blue
		<= Disabled $mol_switch
			value?val <=> color?val \red
			enabled false
			options *
				red <= option_red @ \Red
				green <= option_green @ \Green
				blue <= option_blue @ \Blue

